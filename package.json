{
  "name": "routing-controller-test",
  "version": "0.0.0",
  "description": "TypeScript + Routing Controllers + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts  && npm run copy-static-assets",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-bide\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "copy-static-assets": "ts-node src/copyStaticAssets.ts",
    "dev": "nodemon src/server.ts"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.8.1",
    "@types/pdfkit": "^0.12.6",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^4.6.0",
    "csurf": "^1.11.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.1",
    "mongoose": "^6.3.5",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "pdfkit": "^0.13.0",
    "stripe": "^9.8.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-flash": "^0.0.37",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.35",
    "@types/nodemailer": "^6.4.4",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "shelljs": "^0.8.5",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.2"
  }
}
